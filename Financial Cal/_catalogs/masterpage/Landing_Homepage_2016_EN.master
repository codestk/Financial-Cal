&lt;script type=&quot;text/javascript&quot; language=&quot;javascript&quot;&gt;
var MaximumNumberOfLinks = 5;
var CookieName = &quot;HistoryLinks&quot;;
var DaysToLive = 366;
var HistoryLink = new Array();
var HistoryTitle = new Array();
var HistoryDate = new Array();
var CurrentPage = new String();
var HistoryContent = new String();
DaysToLive = parseInt(DaysToLive);
var varLastVisit;
var searchLink = new Array();

MaximumNumberOfLinks = parseInt(MaximumNumberOfLinks);
if( MaximumNumberOfLinks &lt; 1 ) { MaximumNumberOfLinks = 10; }

function GetCookie() {
                var link = document.URL;
                var title = document.title.length &gt; 1 ? document.title : 'Untitled';              

                var cookiecontent = '';   

                if(document.cookie.length &gt; 0) {
                   var cookiename = CookieName + '&gt;';
                   var cookiebegin = document.cookie.indexOf(cookiename);
                                if(cookiebegin &gt; -1) {
                                                cookiebegin += cookiename.length;
                                                var cookieend = document.cookie.indexOf(&quot;;&quot;,cookiebegin);
                                                if(cookieend &lt; cookiebegin) { cookieend = document.cookie.length; }
                                                cookiecontent = document.cookie.substr(cookiebegin,cookieend);
                                }
                }              

                if( cookiecontent.length &lt; 3 ) { return; }

                cookiecontent = unescape(cookiecontent);     
				var historyList = cookiecontent.split('&amp;');
                                for( var i = 0; i &lt; historyList.length;i++){ 
                                   var link = historyList[i].split('&gt;',3);

                                   HistoryLink.push(link[0]);
                                   HistoryTitle.push(link[1]);
                                   HistoryDate.push(link[2]);
                                   
                                   var temparray;

                                   temparray = link[0].split('~amp;');
                                   link[0] = temparray.join('&amp;');
                                   temparray = link[1].split('~amp;');
                                   link[1] = temparray.join('&amp;');
                                   						   
                                   if (link[1] != title) {
                                   			HistoryContent = '&lt;li&gt;&lt;a href=&quot;'+link[0]+'&quot;&gt;'+link[1]+'&lt;/a&gt;&lt;/li&gt;' + HistoryContent;                
                                   }                             
                                }
}

function RecordCurrentPage() {

                var link = document.URL;
                var title = document.title;
                var searchLinkMedia = link.split('?');		
                var checkPage = searchLinkMedia[0].split('SearchResults.aspx');
                searchLink = searchLinkMedia[0].split('/');

                if ((searchLink[5] != 'Search') &amp;&amp; (checkPage.length &lt; 2)){      
                                var currentTime = new Date();
                                var month = currentTime.getMonth() + 1;
                                var day = currentTime.getDate();
                                var year = currentTime.getFullYear();
                                var hours = currentTime.getHours();
                                var minutes = currentTime.getMinutes();
                                
                                var dateNow =(' '+month + '/' + day + '/' + year + '-' + hours + ':' + minutes);
                                CurrentPage = '&lt;'+'p&gt;'+title+'&lt;'+'/'+'p&gt;';        
                             
                                var temparray = link.split('&amp;');
                                link = temparray.join('~amp;');
                                var temparray = title.split('&amp;');
                                title = temparray.join('~amp;'); 
                
                                for(var i=0;i&lt;HistoryTitle.length;i++){ 
                                                if(HistoryTitle[i] == title){
                                                                HistoryTitle.splice(i,1); 
                                                                HistoryLink.splice(i,1); 
                                                                HistoryDate.splice(i,1); 
                                                }
                                }
                                HistoryLink.push(link);
                                HistoryTitle.push(title);
                                HistoryDate.push(dateNow);     
                }
                                if(document.getElementById('pShowLastVisit') != null){
                                                document.getElementById('pShowLastVisit').innerHTML = HistoryContent;                        
                                }
                
}

function PutCookie() {
	if (searchLink[5] != 'Search'){  
		if( HistoryLink.length &lt; 1 ) { return; }
			var len = HistoryLink.length;
			while( HistoryLink.length &gt; MaximumNumberOfLinks ) {
            	HistoryTitle.shift();
            	HistoryLink.shift();
				HistoryDate.shift();
            }
            var pairs = new Array();
            for( var i = 0; i &lt; HistoryLink.length; i++ ) { pairs.push(HistoryLink[i]+'&gt;'+HistoryTitle[i]+'&gt;'+HistoryDate[i]); }
            	var value = pairs.join('&amp;');
            	var exp = new String();
            	if(DaysToLive &gt; 0) {
            		var now = new Date();
            		now.setTime( now.getTime() + (DaysToLive * 24 * 60 * 60 * 1000) );
            		exp = '; expires=' + now.toGMTString();
            	}
			document.cookie = CookieName + '&gt;' + escape(value) + '; path=/' + exp;           
	}
}

&lt;/script&gt;

