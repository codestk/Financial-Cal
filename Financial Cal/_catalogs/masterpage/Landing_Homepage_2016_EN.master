    <script runat="server">
    	string strTitleName;
		public void Page_Load(Object sender, EventArgs e){
		
			if (HttpContext.Current.Request.IsAuthenticated)	 {
			 	PlaceHolderAuthendicatedLinks.Visible = true;
			 } else {
			 	PlaceHolderAnomymouseLinks.Visible = true;
			 }		 		
	 		string PageTitle;
			this.PlaceHolderPageTitle.Visible = true;
			this.PlaceHolderPageTitleInTitleArea.Visible = true;		
	        if (this.IsPostBack == false) {
		        if ((String.Compare(SPContext.Current.ListItem.Title,"DEFAULT",true) == 0) && (SPContext.Current.ListItem["Comments"] != null)) {
		        	PageTitle = SPContext.Current.ListItem["Comments"].ToString();
		        } else {
		        	PageTitle = SPContext.Current.ListItem.Title;
		        }
		        int loc = PageTitle.ToLower().IndexOf("[");
		        if (loc >= 0) {
		        	strTitleName = PageTitle.Substring(0,loc);
		        } else {
			        strTitleName = PageTitle;
		        }
		    } 
		}
		public String TitleName { get{ return strTitleName as string; } }	
	</script>

